[32m+[m[32mconst P02 = () => {[m
[32m+[m[32m  const headerInfo: TMainHeaderInfoTypes = {[m
[32m+[m[32m    headerText: '[Read] Îã®Ïñ¥ Ïó∞Ïäµ',[m
